	"routeAction": "distribute",
	"singleton": "1",
	"dataFieldName": ["MTw" "PDw" "T1w"],
	"protMap": {
		
		"foreach": "mt",
		
		"MTw":{
		"qMRLabProt": ["FlipAngle" "RepetitionTimeExcitation"],
		"scale": [1 0.001],
		"fillProtBy": "parameter",
		"index": "1"
		},
		
		"PDw":{
		"qMRLabProt": ["FlipAngle" "RepetitionTimeExcitation"],
		"scale": [1 0.001],
		"fillProtBy": "parameter",
		"index": "2"
		},

		"T1w":{
		"qMRLabProt": ["FlipAngle" "RepetitionTimeExcitation"],
		"scale": [1 0.001],
		"fillProtBy": "parameter",
		"index": "2"
		}
	},
	"outputMap": {
		"MTSAT": "MTsat",
		"T1": "T1map"
	},

	"estimationPaper": "Helms, G. et al. (2008). Magn Reson Med 60:1396-1407.",
	"estimationAlgorithm": "src/Models_Functions/MTSATfun/MTSAT_exec.m"
   

}
